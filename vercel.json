{
  "version": 2,
  "builds": [
    {
      "src": "./dist/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/vinos(.*)",
      "dest": "./dist/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    },
    {
      "src": "/api/(.*)",
      "dest": "./dist/main.ts",
      "methods": ["GET", "POST", "PUT", "DELETE"]
    }
  ]
}
